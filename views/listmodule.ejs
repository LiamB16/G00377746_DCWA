<style>
    table, tb, td {
        border: 1px solid black;
    }
</style>
<table>
    <tr>
    <th>mid</th>
    <th>name</th>
    <th>credits</th>
    </tr>
    <% module.forEach((module) =>
    { %>
    <tr>
    <td><%= module.mid %></td>
    <td><%= module.name %></td>
    <td><%= module.credits %></td>
    <td><a href="/updatemodule/<%= module.mid %>">Update</a></td>
    <td><a href="/module_students/<%= module.mid %>">List</a></td>
    </tr>
    <% }) %>
    </table>

    <td><a href="/">home</a></td>